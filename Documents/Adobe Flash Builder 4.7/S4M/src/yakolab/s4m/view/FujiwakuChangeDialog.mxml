<?xml version="1.0" encoding="utf-8"?>
<s:TitleWindow xmlns:fx="http://ns.adobe.com/mxml/2009"
			   xmlns:s="library://ns.adobe.com/flex/spark"
			   xmlns:mx="library://ns.adobe.com/flex/mx"
			   width="400" height="300" close="closeHandler(event)"
			   creationComplete="onCreationComplete(event)"
			   skinClass="spark.skins.spark.TitleWindowSkin" title="譜字枠の変更">	
	<fx:Declarations>
		<!-- 非ビジュアルエレメント (サービス、値オブジェクトなど) をここに配置 -->
	</fx:Declarations>
	<fx:Script source="fujiwakuChangeDialog.as" />
	
	<s:BorderContainer id="gridContainer" x="51" y="38" width="152" height="152" styleName="fujiwakuChangeViewer">
		<mx:UIComponent id="gridUIComponent" x="0" y="0" width="100%" height="100%">
		</mx:UIComponent>
	</s:BorderContainer>
	<s:Button id="button_ok" x="155" y="217" width="86" height="31" label="OK"
			  click="onOKClick(event)"/>
	<s:Button id="button_cancel" x="256" y="216" width="88" height="32" label="キャンセル"
			  click="onCancelClick(event)"/>
	<s:Label x="244" y="39" width="54" text="列数(W):" textAlign="left"/>
	<mx:HSlider id="hslider" x="44" y="21" width="165" liveDragging="true" maximum="100.0"
				minimum="0.0" snapInterval="1" thumbCount="2" change="onSliderChange(event)"/>
	<mx:VSlider id="vslider" x="32" y="32" height="165" liveDragging="true" maximum="100.0"
				minimum="0.0" snapInterval="1" thumbCount="2" change="onSliderChange(event)"/>
	<s:NumericStepper id="rowNW" x="302" y="36" maximum="10" minimum="3" stepSize="1"
					  change="onRowNumberChange(event)"/>
	<s:Label x="244" y="72" width="54" text="行数(H):" textAlign="left"/>
	<s:NumericStepper id="columnNH" x="302" y="70" maximum="10" minimum="3" stepSize="1"
					  change="onColumnNumberChange(event)"/>
	<s:Label x="224" y="128" width="120" color="#5B3E05" text="中心譜字の位置:" textAlign="left"/>
	<s:NumericStepper id="centerNX" x="237" y="153" maximum="10" minimum="1" stepSize="1"
					  change="onCenterPositionChange(event)"/>
	<s:Label x="224" y="156" width="10" text="(" textAlign="right"/>
	<s:Label x="363" y="157" width="15" text=")" textAlign="left"/>
	<s:Label x="293" y="159" width="19" text="," textAlign="left"/>
	<s:NumericStepper id="centerNY" x="306" y="153" maximum="10" minimum="1" stepSize="1"
					  change="onCenterPositionChange(event)"/>
	<mx:HRule x="231" y="108" width="141" height="2"/>
	<mx:HRule x="231" y="22" width="141" height="2"/>
</s:TitleWindow>
